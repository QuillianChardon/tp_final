{% extends "base.html.twig" %}

{% block body %}
	<h2>Ajout de photo</h2>
	{{ form_start(form) }}
	{{ form_errors(form) }}
	<div class="row">
		<div class="col">
			{{ form_row(form.file)}}
		</div>
	</div>
	<button type="submit" class="btn btn-primary mt-2">
		<i class="fas fa-save"></i>&nbsp; Ajouter
	</button>

	{{ form_end(form) }}
{% endblock %}
